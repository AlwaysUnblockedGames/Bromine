{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"allowImportingTsExtensions": "true",
		"baseUrl": ".",
		"paths": {
			"@/components/*": ["src/components/*"],
			"@/utils/*": ["src/utils/*"],
			"@/data/*": ["src/data/*"],
			"@/*": ["src/*"],
			"@/assets/*": ["src/assets/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "preact"
	},
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"]
}
